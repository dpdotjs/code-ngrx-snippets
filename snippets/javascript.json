        "  switch (action.type) {",
        "    case ${ACTIONSNAME}.GET_ALL: {",
        "      return {",
        "        ",
        "      };",
        "    }",
        "    case ${ACTIONSNAME}.GET_ALL_SUCCESS: {",
        "      return {",
        "        ",
        "      };",
        "    }",
        "    case ${ACTIONSNAME}.GET_ALL_FAILURE: {",
        "      return {",
        "        ",
        "      };",
        "    }",
        "    default: {",
        "      return state;",
        "    }",
        "  }",
        "}"
	],
	"description": "Scaffolds an ngrx Reducer."
  }
}